<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.cgv.accelorometer.MainActivity">


    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="115dp"
        android:layout_marginStart="115dp"
        android:layout_marginTop="27dp"
        android:text="Robot Axis"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <TextView
        android:id="@+id/Xdeg"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="40dp"
        android:text="X:"
        android:layout_below="@+id/textView3"
        android:layout_alignRight="@+id/textView3"
        android:layout_alignEnd="@+id/textView3"
        android:layout_marginRight="45dp"
        android:layout_marginEnd="45dp" />

    <TextView
        android:id="@+id/Ydeg"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="17dp"
        android:text="Y:"
        android:layout_below="@+id/Xdeg"
        android:layout_alignLeft="@+id/Xdeg"
        android:layout_alignStart="@+id/Xdeg" />

    <TextView
        android:id="@+id/Zdeg"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="15dp"
        android:text="Z:"
        android:layout_below="@+id/Ydeg"
        android:layout_alignRight="@+id/Xdeg"
        android:layout_alignEnd="@+id/Xdeg" />

    <TextView
        android:id="@+id/Front_I"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Uninclined"
        android:layout_below="@+id/Zdeg"
        android:layout_alignLeft="@+id/textView3"
        android:layout_alignStart="@+id/textView3"
        android:layout_marginTop="38dp" />

</RelativeLayout>
